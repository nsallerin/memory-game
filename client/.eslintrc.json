{
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "jest": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-console": "off",
    "no-alert": "off",
    "comma-dangle": "off",
    "quotes": "off",
    "no-unused-vars": "off",
    "no-param-reassign": "off",
    "no-new": "off",
    "class-methods-use-this": "off",
    "import/prefer-default-export": "off",
    "prefer-destructuring": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }]
  }
}
